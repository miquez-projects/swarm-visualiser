{
  "name": "swarm-visualizer",
  "version": "1.0.0",
  "description": "Web application to visualize Swarm check-in data",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd client && npm run build",
    "test": "cd server && npm test",
    "logs:start": "./scripts/start-log-stream.sh",
    "logs:stop": "./scripts/stop-log-stream.sh",
    "logs:view": "tail -f logs/render-stream.log"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@photostructure/tz-lookup": "^11.3.0",
    "axios": "^1.13.1",
    "csv-parser": "^3.2.0",
    "express-session": "^1.18.2",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-foursquare": "^1.0.0",
    "pg-boss": "^11.1.1"
  }
}
